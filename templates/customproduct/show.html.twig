{% extends 'base.html.twig' %}

{% block title %}Customproduct{% endblock %}

{% block body %}
    <h1>Customproduct</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>CustomProductId</th>
                <td>{{ customproduct.customProductId }}</td>
            </tr>
            <tr>
                <th>Product</th>
                <td>{{ product.name }}</td>
            </tr>
              <tr>
                <th>category</th>
                <td>{{ product.categoryId }}</td>
            </tr>
              <tr>
                <th>Description</th>
                <td>{{ product.description }}</td>
            </tr>
            <tr>
                <th>dimensions</th>
                <td>{{ product.dimensions }}</td>
            </tr>
                <tr>
                <th>weight</th>
                <td>{{ product.weight }}</td>
            </tr>
               <tr>
                <th>material</th>
                <td>{{ product.material }}</td>
            </tr>
                <tr>
                <th>image</th>
                <td>{{ product.image }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_customproduct_index') }}">back to list</a>

    <a href="{{ path('app_customproduct_edit', {'customProductId': customproduct.customProductId}) }}">edit</a>

    {{ include('customproduct/_delete_form.html.twig') }}
{% endblock %}
