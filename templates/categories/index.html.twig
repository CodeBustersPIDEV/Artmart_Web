{% extends 'baseC.html.twig' %} {% import "base.html.twig" as base %}{% block
title %}Categories index{% endblock %} {% block body %}
{% block content %}
{{ base.NavBar() }}
<table class="table">
	<thead>
		<tr>
			<th>CategoriesId</th>
			<th>Name</th>
			<th>actions</th>
		</tr>
	</thead>
	<tbody>
		{% for category in categories %}
		<tr>
			<td>{{ category.categoriesId }}</td>
			<td>{{ category.name }}</td>
			<td>
				<a
					href="{{
						path('app_categories_show', { categoriesId: category.categoriesId })
					}}"
					>show</a
				>
				<a
					href="{{
						path('app_categories_edit', { categoriesId: category.categoriesId })
					}}"
					>edit</a
				>
			</td>
		</tr>
		{% else %}
		<tr>
			<td colspan="3">no records found</td>
		</tr>
		{% endfor %}
	</tbody>
</table>

<a href="{{ path('app_categories_new') }}">Create new</a>
{% endblock %}
{% endblock %}