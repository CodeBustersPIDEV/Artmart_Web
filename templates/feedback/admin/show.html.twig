{% extends 'baseE.html.twig' %} 
{% block content %}
{% block body %}
	{% include('event/admin/eventBar.html.twig') %}

    <h1>Feedback</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Feedbackid</th>
                <td>{{ feedback.feedbackid }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ feedback.rating }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ feedback.comment }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ feedback.date ? feedback.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_feedback_index_admin') }}">back to list</a>

    <a href="{{ path('app_feedback_edit_admin', {'feedbackid': feedback.feedbackid}) }}">edit</a>

    {{ include('feedback/admin/_delete_form.html.twig') }}
{% endblock %}
{% endblock %}
